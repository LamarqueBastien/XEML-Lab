QLineEdit:focus {
	border-width: 2px;
	padding: 0px;
}

QComboBox::item:hover {
	background-color: wheat;
}


QTableView QTableCornerButton::section {
	border-width: 2px;
	border-style: outset;
	border-color: #AAAAAA;
	background-color: #EEEEEE;
}


QTableView QHeaderView::section:checked {
	background-color: #aeaeae;

	color: white;
	font-weight: normal;
}

QTableView {
	 selection-background-color: #D8EFAF;
	 selection-color:black;
}

QComboBox, QLineEdit, QMenu::item:selected, QMenuBar::item:selected   {
selection-background-color: #C0D797;
selection-color:black;
}

QSlider::sub-page:vertical {
background: #89B044;
}

QTabWidget::pane {
	border-style: solid;
	border-width: 2px;
	border-color: #D4D4D4;
	background: white;
	background-image: url(://Images/SilverMetal.png);
}

QTabBar::tab {
	border-style: solid;
	border-width: 1px;
	border-color: #D4D4D4;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #E5E5E5, stop: 0.4 #F0F0F0,stop: 0.5 #E2E2E2, stop: 1.0 #D4D4D4);
	padding: 6px;
	padding-top: 4px;
	padding-bottom: 4px;
}

QTabBar::tab:selected {
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #FFFFFF, stop: 0.4 #F7F7F7,stop: 0.5 #EEEEEE, stop: 1.0 #DDDDDD);
}

QMenuBar {
	background-color: #EEEEEE;
}

QToolBar {
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #EEEEEE, stop: 1.0 lightgrey);
	background-color: #EEEEEE;
}

QStatusBar {
border-style: solid;
border-width: 2px;
border-top-width: 4px;
border-color: lightgrey;
