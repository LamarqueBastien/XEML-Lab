QPushButton {
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #E1E1E1, stop: 0.4 #DDDDDD,stop: 0.5 #D8D8D8, stop: 1.0 #D3D3D3);
		border-style: outset;
		border-width: 1px;
		border-radius: 5px;
		border-color: #AAAAAA;
		font-size:12px;
		font-family:sans;
		font-weight: normal;
	color:black;
	min-width: 8em;
	min-height: 0.95em;
	margin: 0 1px 0 1px;
	color:rgb(0,0,0);
	padding: 6px;
		padding-top: 4px;
		padding-bottom: 4px;
}
QPushButton:hover {
	border-style: outset;
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #F2F2F2, stop: 0.4 #D9D9D9,stop: 0.5 #D7D7D7, stop: 1.0 #C8C8C8);
}


QPushButton:pressed {
	border-style: inset;
	background-color: QLinearGradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #BBBBBB, stop: 0.4 #C8C8C8,stop: 0.5 #CCCCCC, stop: 1.0 #E1E1E1);
}

QPushButton:disabled {
	color: gray;
}

QLabel, QAbstractButton {
	font-weight: normal;
}
